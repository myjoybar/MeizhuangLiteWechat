/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.flex-horizontal {
  width: 100%;
  display: flex;
  flex-direction: row;
}

.flex-vertical {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.flex-horizontal-center {
  width: 100%;
  flex-direction: row;
  justify-content: center;
  display: flex;
}

.flex-vertical-center {
  width: 100%;
  flex-direction: column;
  justify-content: center;
  display: flex;
}

.flex-center {
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  display: flex;
}
.flex-nowrap {
  flex-wrap: nowrap;
   display: flex;
}


.icon_48{
  height: 48rpx;
  width: 48rpx;
  
}
.icon_96{
  height: 96rpx;
  width: 96rpx;
  
}
.page-head-title{
  display: inline-block;
  padding: 0 40rpx 20rpx 40rpx;
  font-size: 32rpx;
  color: #BEBEBE;
}
.page-head-line{
  margin: 0 auto;
  width: 150rpx;
  height: 2rpx;
  background-color: #D8D8D8;
}

.page-foot{
  margin: 100rpx 0 30rpx 0;
  text-align: center;
  color: #1aad19;
  font-size: 0;
}
